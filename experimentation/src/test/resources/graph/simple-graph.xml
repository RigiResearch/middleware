<?xml version="1.0" encoding="UTF-8"?>
<graph xmlns="http://www.rigiresearch.com/middleware/graph/1.0.0">
   <variable name="cores">
      <metadata>
         <property name="cores_min" value="1"/>
         <property name="cores_max" value="6"/>
      </metadata>
   </variable>
   <function name="cost">
      <dependency name="cost-cores" node="cores"/>
      <dependency name="cost-demand" node="demand"/>
      <metadata>
         <property name="equation" value="1*cores"/>
      </metadata>
   </function>
   <variable name="demand">
      <metadata>
         <property name="demand_min" value="0"/>
         <property name="demand_max" value="1000"/>
      </metadata>
   </variable>
   <metric name="latency">
      <dependency name="latency-demand" node="demand"/>
      <dependency name="latency-cores" node="cores"/>
      <metadata>
         <property name="type" value="minimize"/>
      </metadata>
   </metric>
</graph>

