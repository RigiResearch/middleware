<?xml version="1.0" encoding="UTF-8"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
  package-name="com.rigiresearch.middleware.graph">
  <java-types>
    <java-type name="Graph">
      <java-attributes>
        <xml-elements java-attribute="nodes">
          <xml-element name="variable" type="com.rigiresearch.middleware.experimentation.graph.Variable"/>
          <xml-element name="metric" type="com.rigiresearch.middleware.experimentation.graph.Metric"/>
          <xml-element name="function" type="com.rigiresearch.middleware.experimentation.graph.Function"/>
        </xml-elements>
      </java-attributes>
    </java-type>
  </java-types>
</xml-bindings>
